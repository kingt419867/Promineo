package test02_conditionals;
import java.io.*;
import java.util.*;
public class BasicLooping {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i;
//		for (i=0; i<=100; i = (i*2 +1)) {
//			System.out.println(i);
//		}
		
        int userValue = 2;
        for (i = 1; i >= 100; i += userValue) {
            System.out.println(i);
        }
	}

}
